{
  "name": "ai-sdk-provider-poe",
  "version": "0.0.0-dev",
  "type": "module",
  "main": "./dist/stable/index.js",
  "types": "./dist/stable/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/stable/index.d.ts",
      "import": "./dist/stable/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsx scripts/build-release-stages.ts",
    "test": "RELEASE_STAGE=alpha vitest run",
    "test:stable": "RELEASE_STAGE=stable vitest run",
    "test:beta": "RELEASE_STAGE=beta vitest run",
    "test:alpha": "RELEASE_STAGE=alpha vitest run",
    "test:watch": "vitest",
    "snapshots:list": "tsx scripts/snapshots.ts list",
    "snapshots:list:stale": "tsx scripts/snapshots.ts list:stale",
    "snapshots:delete": "tsx scripts/snapshots.ts delete",
    "snapshots:delete:stale": "tsx scripts/snapshots.ts delete:stale",
    "verify-model": "tsx scripts/verify-model.ts",
    "verify-model:stable": "RELEASE_STAGE=stable tsx scripts/verify-model.ts",
    "verify-model:beta": "RELEASE_STAGE=beta tsx scripts/verify-model.ts",
    "verify-model:alpha": "RELEASE_STAGE=alpha tsx scripts/verify-model.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.37",
    "@ai-sdk/openai": "^3.0.25",
    "@ai-sdk/provider": "^3.0.7",
    "@ai-sdk/provider-utils": "^4.0.13"
  },
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "ai": "^6.0.68",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.1.0-beta.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poe-platform/ai-sdk-provider-poe.git"
  }
}
